<div id="table-8" class="list colspan-8">
  <div class="col-8 no-border left">
    <h3>Equipe de trabalho</h3>
  </div>

  <div class="col-8 list header">
    <div class="col-1">Ciclos</div>
    <div class="col-2">Pessoas da Equipe</div>
    <div class="col-1">Nº de Visitas</div>
    <div class="col-1">Duração média das visitas</div>
    <div class="col-3">Núcleo de consulta</div>
  </div>

  <% (0..3).each do |c| %>
    <div class="col-8 list hover">
      <div class="col-1 header">{{growth_profiles[<%= c %>].gregorian_cycle}}</div>
      <% {team_people: 2, visits_count: 1, visit_mean_duration: 1, consultation_center: 3}.each do |k, v| %>
        <div class="col-<%= v %>">
          <% if c < 3 %>
            &nbsp;{{growth_profiles[<%= c %>].<%= k %>}}&nbsp;
          <% else %>
            <input ng-change="changeFormState()" ng-model="growth_profiles[3].<%= k %>">
          <% end %>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
